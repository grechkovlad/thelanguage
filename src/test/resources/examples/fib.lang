class Fib {

    private static int fib(int n) {
        if (n == 0) {
            return 0;
        }
        int[] fibs = new int[n + 1];
        fibs[1] = 1;
        for(int i = 2; i <= n; i++) {
            fibs[i] = fibs[i - 2] + fibs[i - 1];
        }
        return fibs[n];
    }

    public static void main(String[] args) {
        System.consolePrint(fib(Utils.parseInt(args[0])));
    }
}